{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "version": {
      "type": "integer"
    },
    "queue": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "is_remote": {
            "type": "boolean"
          },
          "path": {
            "type": "string"
          },
          "schema": {
            "type": "string"
          },
          "method": {
            "type": "string"
          },
          "headers": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": {
                  "type": "string"
                },
                "value": {
                  "type": "string"
                }
              },
              "required": [
                "key",
                "value"
              ]
            }
          }
        },
        "required": [
          "is_remote",
          "path",
          "schema"
        ]
      }
    }
  },
  "required": [
    "version",
    "queue"
  ]
}